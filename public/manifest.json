{
    "manifest_version": 2,
    "version": "0.0.1",
    "short_name": "React App",
    "name": "Load my app",
    "icons": {
        "64": "logo192.png",
        "128": "logo512.png"
    },
    "browser_action": {
        "default_popup": "./index.html"
    },
    "start_url": ".",
    "display": "standalone",
    "theme_color": "#000000",
    "background_color": "#ffffff",
    "permissions": [
        "cookies",
        "webRequest",
        "storage",
        "https://*/",
        "http://*/"
    ]
}
